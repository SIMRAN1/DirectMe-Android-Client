<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.silab.direct_me">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"

        android:theme="@style/MyMaterialTheme">

        <activity android:name=".Activity.MainActivity" android:screenOrientation="landscape">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Dashboard" android:screenOrientation="landscape"
            >
            <intent-filter>
                <action android:name="in.silive.directme.DASHBOARD" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Show_room" android:screenOrientation="landscape"
            >
            <intent-filter>
                <action android:name="in.silive.directme.SHOW_ROOM" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Dockyard" android:screenOrientation="landscape"
            >
            <intent-filter>
                <action android:name="in.silive.directme.DOCKYARD" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.CreateAccount" android:screenOrientation="landscape"
            >
            <intent-filter>
                <action android:name="in.silive.directme.GARAGE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.UserLogin" android:screenOrientation="landscape"
            >
            <intent-filter>
                <action android:name="in.silive.directme.GARAGE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Parknow" android:screenOrientation="landscape"
            android:parentActivityName=".Activity.Dashboard">
            <intent-filter>
                <action android:name="in.silive.directme.PARKNOW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Parked" android:screenOrientation="landscape"
            android:parentActivityName=".Activity.Dashboard">
            <intent-filter>
                <action android:name="in.silive.directme.PARKNOW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.enter_db" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="in.silive.directme.ENTER_DB" />
                <category android:name="android.intent.category.DEFAULT" />

            </intent-filter>
        </activity>



    <activity android:name=".Activity.Login" android:screenOrientation="landscape">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />


        </intent-filter>
    </activity>
        <activity android:name=".Activity.Parkinge" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />


            </intent-filter>
        </activity>

    <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_appid_"/>

        <service android:name=".Service.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name=".Service.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

</application>


</manifest>