<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent">
    >

    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="horizontal"
        android:layout_width="0dp"
        android:background="@drawable/img"
        android:layout_height="0dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintHorizontal_bias="0.66">

        <RelativeLayout
            android:layout_width="0dp"
            android:background="#FFFFFF"
            android:elevation="2dp"
            android:layout_height="0dp"
            android:id="@+id/gold_show"
            app:layout_constraintRight_toLeftOf="@+id/banana_show"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/banana_show"
            android:layout_marginEnd="6dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="@+id/banana_show"
            app:layout_constraintLeft_toRightOf="@+id/showroom"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="6dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/coin"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:id="@+id/imageView8" />

            <TextView
                android:text="0000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="8dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:id="@+id/gold_no" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="76dp"
            android:background="#FFFFFF"
            android:layout_height="0dp"
            android:id="@+id/banana_show"
            android:elevation="2dp"
            app:layout_constraintRight_toLeftOf="@+id/bamboo_show"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="82dp"
            app:layout_constraintBottom_toBottomOf="@+id/coconut_show"
            android:layout_marginEnd="82dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="@+id/coconut_show"
            app:layout_constraintLeft_toRightOf="@+id/showroom"
            android:layout_marginLeft="82dp"
            android:layout_marginRight="82dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/banana"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/imageView4" />

            <TextView
                android:text="0000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="8dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:id="@+id/banana_no" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="76dp"
            android:background="#FFFFFF"
            android:layout_height="0dp"
            android:id="@+id/coconut_show"
            android:elevation="2dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="79dp"
            app:layout_constraintBottom_toBottomOf="@+id/bamboo_show"
            android:layout_marginEnd="79dp"
            app:layout_constraintRight_toRightOf="@+id/bamboo_show"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/banana_show"
            app:layout_constraintTop_toTopOf="@+id/bamboo_show"
            android:layout_marginLeft="79dp"
            android:layout_marginRight="79dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/coconut"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/imageView5" />

            <TextView
                android:text="0000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="8dp"
                android:id="@+id/coconut_no" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="76dp"
            android:background="#FFFFFF"
            android:layout_height="0dp"
            android:elevation="2dp"
            android:id="@+id/bamboo_show"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/timber_show"
            android:layout_marginEnd="165dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/timber_show"
            android:layout_marginRight="165dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/bamboo"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:id="@+id/imageView6" />

            <TextView
                android:text="0000"
                android:layout_marginRight="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:id="@+id/bamboo_no" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="76dp"
            android:background="#FFFFFF"
            android:layout_height="30dp"
            android:elevation="2dp"
            android:id="@+id/timber_show"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/relativeLayout"
            android:layout_marginStart="84dp"
            android:layout_marginEnd="84dp"
            app:layout_constraintRight_toRightOf="@+id/relativeLayout"
            android:layout_marginTop="12dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="10dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/coconut_show"
            android:layout_marginLeft="84dp"
            android:layout_marginRight="84dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/wood"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:id="@+id/imageView7" />

            <TextView
                android:text="0000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="8dp"
                android:id="@+id/wood_no" />
        </RelativeLayout>

        <RelativeLayout
            android:text="Button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="2dp"
            android:background="#006400"
            android:id="@+id/button"
            app:layout_constraintRight_toLeftOf="@+id/showroom"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/showroom"
            android:layout_marginEnd="6dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/showroom"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="6dp" />

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/showroom"
            android:background="#FFFFFF"
            android:elevation="2dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="31dp"
            app:layout_constraintBottom_toBottomOf="@+id/gold_show"
            android:layout_marginEnd="31dp"
            app:layout_constraintRight_toRightOf="@+id/boatchar"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/boatchar"
            app:layout_constraintTop_toTopOf="@+id/gold_show"
            android:layout_marginLeft="31dp"
            android:layout_marginRight="31dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="PARKING" />
        </RelativeLayout>


        <android.support.constraint.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="#78676767"
            android:id="@+id/relativeLayout"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="52dp"
            app:layout_constraintLeft_toLeftOf="@+id/banana_show"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="6dp"
            android:layout_marginTop="52dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/boatimage"
                android:src="@drawable/island1"
                tools:layout_constraintTop_creator="1"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="PARKING DETAILS"
                android:id="@+id/details"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"

                android:layout_marginStart="18dp"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/boatimage"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/boatimage" />

            <TextView
                android:layout_width="73dp"
                android:layout_height="wrap_content"
                android:text="USER NAME"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"

                android:id="@+id/owner"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/details"
                app:layout_constraintLeft_toLeftOf="@+id/details" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-NA-"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"

                android:id="@+id/odetail"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toRightOf="@+id/owner"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/details"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="BOAT NAME"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"

                android:id="@+id/active"
                tools:layout_constraintRight_creator="1"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/parkingslot"
                app:layout_constraintRight_toRightOf="@+id/owner"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="10dp"
                app:layout_constraintLeft_toLeftOf="@+id/owner"
                app:layout_constraintTop_toBottomOf="@+id/owner"
                android:layout_marginTop="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-NA-"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"

                android:id="@+id/odet"
                tools:layout_constraintRight_creator="1"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/sdet"
                app:layout_constraintRight_toRightOf="@+id/odetail"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="10dp"
                app:layout_constraintLeft_toLeftOf="@+id/odetail"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/odetail" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ISLAND"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"

                android:id="@+id/parkingslot"
                tools:layout_constraintTop_creator="1"
                app:layout_constraintTop_toBottomOf="@+id/active"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/active"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.0"
                android:layout_marginTop="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-NA-"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"

                android:id="@+id/sdet"
                tools:layout_constraintTop_creator="1"
                tools:layout_constraintRight_creator="1"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toBottomOf="@+id/parkingslot"
                app:layout_constraintRight_toRightOf="@+id/odet"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/odet"
                app:layout_constraintTop_toTopOf="@+id/parkingslot" />

        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#78676767"
            android:id="@+id/boatchar"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="17dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="52dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="52dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="17dp">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="BOAT"

                android:fontFamily="sans-serif-condensed"
                android:id="@+id/boatname"
                android:textColor="#ffffff"
                android:textSize="40sp"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/chars"
                android:layout_marginStart="6dp"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="5dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView2"
                android:layout_marginLeft="6dp"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Characterstics"
                android:id="@+id/chars"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="20sp"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/capacity"
                android:layout_marginStart="30dp"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="10dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="30dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Capacity"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:id="@+id/capacity"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/fill"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="8dp"
                app:layout_constraintLeft_toLeftOf="@+id/chars" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Capacityvalue"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/capacityval"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/fill"
                android:layout_marginStart="16dp"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="10dp"
                app:layout_constraintLeft_toRightOf="@+id/capacity"
                android:layout_marginLeft="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Filling speed"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:id="@+id/fill"
                app:layout_constraintBaseline_toBaselineOf="@+id/fillval"
                tools:layout_constraintBaseline_creator="1"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/capacity" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="15m/s"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/fillval"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/time"
                android:layout_marginStart="9dp"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="12dp"
                app:layout_constraintLeft_toRightOf="@+id/fill"
                android:layout_marginLeft="9dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Parking Time"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:id="@+id/time"
                app:layout_constraintBaseline_toBaselineOf="@+id/timeal"
                tools:layout_constraintBaseline_creator="1"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/boatfill" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1:00 hr"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/timeal"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/fillvalue"
                android:layout_marginStart="4dp"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="16dp"
                app:layout_constraintLeft_toRightOf="@+id/time"
                android:layout_marginLeft="4dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Boat Filled"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:id="@+id/boatfill"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/coins"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="10dp"
                app:layout_constraintLeft_toLeftOf="@+id/coins" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="20%"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/fillvalue"
                tools:layout_constraintTop_creator="1"
                tools:layout_constraintBottom_creator="1"
                android:layout_marginStart="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/boatfill"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/timeal"
                app:layout_constraintTop_toTopOf="@+id/boatfill"
                android:layout_marginLeft="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Coins Earn"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:id="@+id/coins"
                tools:layout_constraintBottom_creator="1"
                android:layout_marginStart="30dp"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="30dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="20"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/coinvalue"
                tools:layout_constraintTop_creator="1"
                tools:layout_constraintRight_creator="1"
                android:layout_marginEnd="3dp"
                app:layout_constraintRight_toRightOf="@+id/fillvalue"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/fillvalue"
                android:layout_marginRight="3dp" />

            <ImageView
                android:layout_width="49dp"
                android:layout_height="0dp"
                app:srcCompat="@drawable/ship1"
                android:id="@+id/imageView2"
                app:layout_constraintRight_toLeftOf="@+id/capacityval"
                tools:layout_constraintRight_creator="1"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/chars"
                android:layout_marginStart="25dp"
                android:layout_marginEnd="23dp"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="3dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="23dp"
                app:layout_constraintTop_toTopOf="parent" />

        </android.support.constraint.ConstraintLayout>


        <Button
            android:text="Undock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/undocker"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout"
            android:layout_marginLeft="11dp"
            android:layout_marginStart="11dp"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout"
            android:layout_marginTop="16dp" />


    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>