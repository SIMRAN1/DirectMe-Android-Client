<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent">
    >

    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="horizontal"
        android:layout_width="0dp"
        android:background="@drawable/img"
        android:layout_height="0dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintHorizontal_bias="0.66">

        <android.support.constraint.ConstraintLayout
            android:layout_width="wrap_content"
            android:background="#FFFFFF"
            android:elevation="2dp"
            android:layout_height="wrap_content"
            android:id="@+id/gold_show"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/showroom"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/coin"
                android:id="@+id/imageView8"

                tools:layout_constraintTop_creator="1"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" />

            <TextView
                android:text="0000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gold_no"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="@+id/imageView8"
                app:layout_constraintLeft_toRightOf="@+id/imageView8"
                android:layout_marginStart="4dp"
                android:layout_marginLeft="4dp"
                />
        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:layout_width="wrap_content"
            android:background="#FFFFFF"
            android:layout_height="wrap_content"
            android:id="@+id/banana_show"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/gold_show"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:srcCompat="@drawable/banana"
                android:id="@+id/imageView4"
                tools:layout_constraintTop_creator="1"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" />

            <TextView
                android:text="0000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/banana_no"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="@+id/imageView4"
                app:layout_constraintLeft_toRightOf="@+id/imageView4"
                android:layout_marginStart="4dp"
                android:layout_marginLeft="4dp" />
        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:layout_width="wrap_content"
            android:background="#FFFFFF"
            android:layout_height="wrap_content"
            android:id="@+id/coconut_show"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/banana_show"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:srcCompat="@drawable/coconut"
                android:id="@+id/imageView5"
                tools:layout_constraintTop_creator="1"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" />

            <TextView
                android:text="0000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/coconut_no"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="@+id/imageView5"
                app:layout_constraintLeft_toRightOf="@+id/imageView5"
                android:layout_marginStart="4dp"
                android:layout_marginLeft="4dp" />
        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:layout_width="wrap_content"
            android:background="#FFFFFF"
            android:layout_height="wrap_content"

            android:id="@+id/bamboo_show"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/coconut_show"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:srcCompat="@drawable/bamboo"
                android:id="@+id/imageView6"
                tools:layout_constraintTop_creator="1"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" />

            <TextView
                android:text="0000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@+id/imageView6"
                app:layout_constraintLeft_toRightOf="@+id/imageView6"
                android:id="@+id/bamboo_no"
                android:layout_marginTop="8dp"
                android:layout_marginStart="4dp"
                android:layout_marginLeft="4dp" />
        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:layout_width="wrap_content"
            android:background="#FFFFFF"
            android:layout_height="wrap_content"
            android:elevation="2dp"
            android:id="@+id/timber_show"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/bamboo_show"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:srcCompat="@drawable/wood"
                android:id="@+id/imageView7"
                tools:layout_constraintTop_creator="1"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" />

            <TextView
                android:text="0000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/wood_no"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="@+id/imageView7"
                app:layout_constraintLeft_toRightOf="@+id/imageView7"
                android:layout_marginStart="4dp"
                android:layout_marginLeft="4dp" />
        </android.support.constraint.ConstraintLayout>

        <RelativeLayout
            android:text="Button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="2dp"
            android:background="#006400"
            android:id="@+id/button"
            app:layout_constraintRight_toLeftOf="@+id/showroom"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/showroom"
            android:layout_marginEnd="6dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/showroom"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="6dp" />

        <android.support.constraint.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/showroom"
            android:background="#FFFFFF"
            android:elevation="2dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="31dp"
            app:layout_constraintBottom_toBottomOf="@+id/gold_show"
            android:layout_marginEnd="31dp"
            app:layout_constraintRight_toRightOf="@+id/boatchar"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/boatchar"
            app:layout_constraintTop_toTopOf="@+id/gold_show"
            android:layout_marginLeft="31dp"
            android:layout_marginRight="31dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:text="PARKING"
                tools:layout_constraintTop_creator="1"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="8dp" />
        </android.support.constraint.ConstraintLayout>


        <android.support.constraint.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="#78676767"
            android:id="@+id/relativeLayout"
            app:layout_constraintTop_toTopOf="@+id/boatchar"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="6dp"
            app:layout_constraintBottom_toBottomOf="@+id/boatchar"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toRightOf="@+id/boatchar"
            android:layout_marginLeft="6dp"

            app:layout_constraintHorizontal_bias="0.0"

            android:layout_marginRight="8dp"
            android:layout_marginEnd="8dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/boatimage"
                android:src="@drawable/island1"
                tools:layout_constraintTop_creator="1"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="PARKING DETAILS"
                android:id="@+id/details"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"

                android:layout_marginStart="18dp"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/boatimage"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/boatimage" />

            <TextView
                android:layout_width="73dp"
                android:layout_height="wrap_content"
                android:text="USER NAME"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"

                android:id="@+id/owner"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/details"
                app:layout_constraintLeft_toLeftOf="@+id/details" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-NA-"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"

                android:id="@+id/odetail"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toRightOf="@+id/owner"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/details"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="BOAT NAME"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"

                android:id="@+id/active"
                tools:layout_constraintRight_creator="1"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/parkingslot"
                app:layout_constraintRight_toRightOf="@+id/owner"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="10dp"
                app:layout_constraintLeft_toLeftOf="@+id/owner"
                app:layout_constraintTop_toBottomOf="@+id/owner"
                android:layout_marginTop="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-NA-"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"

                android:id="@+id/odet"
                tools:layout_constraintRight_creator="1"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toTopOf="@+id/sdet"
                app:layout_constraintRight_toRightOf="@+id/odetail"
                tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="10dp"
                app:layout_constraintLeft_toLeftOf="@+id/odetail"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/odetail" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ISLAND"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"

                android:id="@+id/parkingslot"
                tools:layout_constraintTop_creator="1"
                app:layout_constraintTop_toBottomOf="@+id/active"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/active"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.0"
                android:layout_marginTop="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-NA-"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"

                android:id="@+id/sdet"
                tools:layout_constraintTop_creator="1"
                tools:layout_constraintRight_creator="1"
                tools:layout_constraintBottom_creator="1"
                app:layout_constraintBottom_toBottomOf="@+id/parkingslot"
                app:layout_constraintRight_toRightOf="@+id/odet"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/odet"
                app:layout_constraintTop_toTopOf="@+id/parkingslot" />

        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#78676767"
            android:id="@+id/boatchar"

            tools:layout_constraintLeft_creator="1"

            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="17dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/showroom"
            android:layout_marginStart="17dp">

            <ImageView
                android:layout_width="49dp"
                android:layout_height="0dp"
                app:srcCompat="@drawable/ship1"
                android:id="@+id/imageship"
                android:layout_marginStart="25dp"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="25dp"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="BOAT"
                app:layout_constraintLeft_toRightOf="@+id/imageship"
                android:fontFamily="sans-serif-condensed"
                android:id="@+id/boatname"
                android:textColor="#ffffff"
                android:textSize="40sp"

                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="4dp"
                android:layout_marginLeft="4dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Characterstics"
                android:id="@+id/chars"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="20sp"
                app:layout_constraintTop_toBottomOf="@+id/boatname"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"
                android:layout_marginTop="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Capacity"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:layout_marginLeft="5dp"
                android:id="@+id/capacity"
                app:layout_constraintTop_toBottomOf="@+id/chars"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginStart="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Capacityvalue"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/capacityval"
                app:layout_constraintTop_toTopOf="@+id/capacity"
                app:layout_constraintBottom_toBottomOf="@+id/capacity"
                app:layout_constraintLeft_toRightOf="@+id/capacity"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Filling speed"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:id="@+id/fill"
                app:layout_constraintTop_toBottomOf="@+id/capacity"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/capacity"
                android:layout_marginTop="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="15m/s"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/fillval"
                app:layout_constraintTop_toTopOf="@+id/fill"
                app:layout_constraintBottom_toBottomOf="@+id/fill"
                app:layout_constraintLeft_toRightOf="@+id/fill"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Parking Time"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:id="@+id/time"
                app:layout_constraintTop_toBottomOf="@+id/fill"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/fill"
                android:layout_marginTop="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1:00 hr"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/timeal"
                app:layout_constraintTop_toTopOf="@+id/time"
                app:layout_constraintBottom_toBottomOf="@+id/time"
                app:layout_constraintLeft_toRightOf="@+id/time"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Boat Filled"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:id="@+id/boatfill"
                app:layout_constraintTop_toBottomOf="@+id/time"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/time"
                android:layout_marginTop="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="20%"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/fillvalue"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintTop_toTopOf="@+id/boatfill"
                app:layout_constraintBottom_toBottomOf="@+id/boatfill"
                app:layout_constraintLeft_toRightOf="@+id/boatfill"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                tools:layout_constraintTop_creator="1"
                tools:layout_constraintBottom_creator="1" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Coins Earn"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:id="@+id/coins"
                app:layout_constraintTop_toBottomOf="@+id/boatfill"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintLeft_toLeftOf="@+id/boatfill"
                android:layout_marginTop="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="20"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/coinvalue"
                app:layout_constraintTop_toTopOf="@+id/coins"
                app:layout_constraintBottom_toBottomOf="@+id/coins"
                app:layout_constraintLeft_toRightOf="@+id/coins"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Experience Gain"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:id="@+id/Xp"

                app:layout_constraintLeft_toLeftOf="@+id/coins"
                app:layout_constraintTop_toBottomOf="@+id/coins"

                app:layout_constraintVertical_bias="1.0"
                android:layout_marginTop="8dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#ff0000"
                android:textSize="10sp"
                android:id="@+id/Xpvalue"
                app:layout_constraintTop_toTopOf="@+id/Xp"
                app:layout_constraintBottom_toBottomOf="@+id/Xp"
                app:layout_constraintLeft_toRightOf="@+id/Xp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" />






        </android.support.constraint.ConstraintLayout>


        <Button
            android:text="Undock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/undocker"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout"
            android:layout_marginLeft="11dp"

            app:layout_constraintTop_toBottomOf="@+id/relativeLayout"
            android:layout_marginTop="16dp"
            android:layout_marginStart="11dp" />


    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>